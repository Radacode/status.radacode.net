{
  "name": "status.radacode.net",
  "private": true,
  "version": "0.0.1",
  "description": "A status report for Radacode Infrastructure availability",
  "repository": "https://github.com/Radacode/status.radacode.net",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "html-webpack-plugin": "^2.22.0",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.8",
    "karma-junit-reporter": "^0.4.1",
    "protractor": "^3.2.2",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "scripts": {
    "prestart": "npm install",
    "start": "webpack-dev-server  --history-api-fallback --debug --hot --devtool eval-source-map --output-pathinfo --watch --colors --inline --content-base public --port 3030 --host 0.0.0.0",
    "pretest": "npm install",
    "build": "webpack -p",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js"
  },
  "dependencies": {
    "angular": "^1.5.7",
    "angular-ui-router": "^0.3.1"
  }
}
